algoritmo "aula 1E - primeiro algoritmo"

var
   nome, sobrenome, nomeCompleto: caractere

inicio
   escreva("Informe seu nome: ")
   leia(nome)

   escreva("Informe seu sobrenome: ")
   leia(sobrenome)
   
   nomeCompleto <- nome + " " + sobrenome
   
   escreval(nomeCompleto)
fimalgoritmo
